PORT=8080
CORS_ORIGIN=http://localhost:3000
APP_VERSION=0.1.0
BUILD_TIME=2025-12-25T00:00:00Z
LOG_LEVEL=info

# TypeORM (호스트에서 실행하면 localhost 권장)
DB_HOST=db
DB_PORT=3306
DB_USER=app
DB_PASSWORD=app
DB_NAME=portfolio

# Redis (호스트에서 실행하면 localhost 권장)
REDIS_URL=redis://redis:6379
# Prisma (추가 필수)
DATABASE_URL="mysql://app:app@db:3306/portfolio"

JWT_ACCESS_SECRET=change_me
JWT_REFRESH_SECRET=change_me
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=14d

# GitHub OAuth
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI=http://localhost:8080/api/auth/oauth/github/callback

# Firebase Admin
# 서비스 계정 JSON을 한 줄로 넣는다(개행 제거)
